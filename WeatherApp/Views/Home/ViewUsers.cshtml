@{
}
@model List<WeatherApp.Models.UserModel>;

<html>
    <head>
        <title>View Users</title>
    </head>
<body>

<h1>All Users</h1>
<div class="row">
    <div class="col-md-12">
        <form asp-action="ViewUsers">
           <table class="table">
              <thead>
                 <tr>
                  <th>#</th>
                  <th>Name</th>
                  <th>Email</th>
                 </tr>
              </thead>
       
   <tbody>
        @foreach (WeatherApp.Models.UserModel item in (dynamic)Model)
        {
      <tr>
              <td>@item.User_ID</td>
              <td>@item.username</td>
              <td>@item.email</td>
      </tr>
        }
  </tbody>

</table>
        </form>
    </div>
    </div> 
</body>
</html>